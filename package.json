{
  "name": "attakk-store-backend",
  "description": "Lightweight REST API for temporary e-commerce campaign",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "node --test tests/**/*.test.ts",
    "code": "npm run typecheck && npm run lint && npm run format",
    "code:fix": "npm run typecheck && npm run lint:fix && npm run format:fix",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "keywords": [
    "express",
    "api",
    "sqlite"
  ],
  "author": "Julian Visser",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.5",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "express": "^4.21.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  }
}
