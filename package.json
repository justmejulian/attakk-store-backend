{
  "name": "attakk-store-backend",
  "version": "1.0.0",
  "description": "Lightweight REST API for temporary e-commerce campaign",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "node --test tests/**/*.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "express",
    "api",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.5",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.50.1"
  },
  "dependencies": {
    "express": "^4.21.2",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  }
}
